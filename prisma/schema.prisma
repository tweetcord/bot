datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

model User {
  id String @id @map("_id")
  donator Boolean @default(false)
  premium Boolean @default(false)
  premium_servers String[] 
}

